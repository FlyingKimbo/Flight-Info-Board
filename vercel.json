{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/save-flight-state", "dest": "/api/flight-progress.js" },
    { "src": "/api/saved-flight-state", "dest": "/api/flight-progress.js" },
    { "src": "/api/update-flight", "dest": "/api/update-flight.js" },
    { "src": "/(.*)", "dest": "/" }
  ]
}
